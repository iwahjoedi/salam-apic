{
    "name": "iwahjoedi/salam-apic",
    "description": "Headless CMS",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Indra Wahjoedi",
            "email": "indra.wahjoedi@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "kint-php/kint": "^3.3",
        "psr/log": "^1.1",
        "laminas/laminas-escaper": "^2.6.1",
        "guzzlehttp/guzzle": "^7.2.0",
        "monken/cli-create": "^0.3.1",
        "tatter/themes": "1.0.3"
    },
    "require-dev": {
        "codeigniter4/codeigniter4-standard": "^1.0",
        "mikey179/vfsstream": "1.6.*",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.3",
        "wensleydale/spark": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CodeIgniter\\": "system/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@composer dump-autoload",
            "CodeIgniter\\ComposerScripts::postUpdate"
        ]
    },
    "support": {
        "forum": "http://forum.codeigniter.com/",
        "source": "https://github.com/iwahjoedi/salam-apic",
        "slack": "https://codeigniterchat.slack.com"
    }
}
